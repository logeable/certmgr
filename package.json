{
  "name": "certmgr",
  "version": "1.0.0",
  "main": "./dist/main/main.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "dev": "ts-node src/scripts/dev.ts",
    "build": "vite build && tsc -p .",
    "start": "npm run build && electron-forge start",
    "make": "npm run build && electron-forge make",
    "package": "npm run build && electron-forge package",
    "format": "prettier --write \"src/**/*.{js,ts,tsx,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,ts,tsx,jsx,json,css,md}\"",
    "lint": "eslint \"{src,main,scripts}/**/*.{js,ts,tsx,jsx}\"",
    "lint:fix": "eslint --fix \"{src,main,scripts}/**/*.{js,ts,tsx,jsx}\""
  },
  "author": "logeable",
  "license": "Apache-2.0",
  "description": "CertMgr is a tool for managing certificates.",
  "devDependencies": {
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-dmg": "^7.8.1",
    "@electron-forge/maker-rpm": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@electron/fuses": "^1.8.0",
    "@eslint/css": "^0.8.1",
    "@eslint/js": "^9.27.0",
    "@eslint/json": "^0.12.0",
    "@types/node": "^22.15.19",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitejs/plugin-react": "^4.4.1",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^36.2.1",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "prettier": "^3.5.3",
    "sass-embedded": "^1.89.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "antd": "^5.25.2",
    "electron-squirrel-startup": "^1.0.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "winston": "^3.17.0"
  }
}
