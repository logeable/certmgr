{
  "name": "certmgr",
  "version": "1.0.0",
  "main": "./dist/main/main.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "dev": "npm run build && concurrently -k \"vite\" \"cross-env NODE_ENV=development electron .\"",
    "build": "vite build && tsc -p .",
    "start": "electron .",
    "make": "electron-forge make"
  },
  "author": "logeable",
  "license": "Apache-2.0",
  "description": "CertMgr is a tool for managing certificates.",
  "devDependencies": {
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-rpm": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron/fuses": "^1.8.0",
    "@types/node": "^22.15.19",
    "@vitejs/plugin-react": "^4.4.1",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^36.2.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  }
}